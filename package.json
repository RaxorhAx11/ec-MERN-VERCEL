{
  "name": "ec-mern-vercel",
  "version": "1.0.0",
  "description": "E-commerce MERN stack application deployed on Vercel",
  "main": "index.js",
  "scripts": {
    "build": "cd client && npm run build",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "cd server && npm run dev",
    "client": "cd client && npm run dev",
    "start": "cd server && npm start",
    "install-all": "npm install && cd client && npm install && cd ../server && npm install"
  },
  "dependencies": {
    "@google-cloud/dialogflow": "^7.1.0",
    "@paypal/paypal-server-sdk": "^1.1.0",
    "bcryptjs": "^2.4.3",
    "cloudinary": "^2.7.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dialogflow": "^1.2.0",
    "dotenv": "^16.6.1",
    "express": "^4.21.2",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.17.1",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^7.0.6"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.1.10"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "mern",
    "ecommerce",
    "vercel",
    "serverless",
    "mongodb",
    "react"
  ],
  "author": "sangam mukherjee",
  "license": "ISC"
}
